{
  "name": "cos-language-support",
  "displayName": "cos-language-support",
  "description": "cos-language-support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "languages": [{
      "id": "cos",
      "aliases": ["COS", "cos"],
      "extensions": [".cos"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cos",
      "scopeName": "source.cos",
      "path": "./syntaxes/cos.json"
    }],
    "snippets": [{
      "language": "cos",
      "path": "./snippets/cos.json"
    }],
    "commands": [{
      "command": "cos.completion",
      "title": "COS Language Completion"
    }],
    "themes": [{
      "label": "COS Theme",
      "uiTheme": "vs-dark",
      "path": "./themes/cos-theme.json"
    }]
  },
  "activationEvents": [
    "onLanguage:cos"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/node": "^16.11.7",
    "typescript": "^4.5.5"
  }
}
