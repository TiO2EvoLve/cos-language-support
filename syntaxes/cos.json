{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "COS",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#commands"
		},
		{
			"include": "#variables"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.cos",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.cos",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.cos",
					"match": "\\\\."
				}
			]
		},
		"commands": {
			"patterns": [
				{
					"name": "support.function.cos",
					"match": "\\b(APDU|RESET|DEF|SUB|CONNECT|EXTERNAL|APDUMAC|APDUKEYE|DESEN|DESDE|XOR|DIVK|MD5|SM4EN|SM4DE|SHA1|SQL_EXIST|SQL_NOEXIST|SQL_CHANGE|M1LOADKEY|M1AUTHEN|M1WRITE|M1READ)\\.>"
				}
			]
		},
		"variables": {
			"patterns": [
				{
					"name": "variable.other.cos",
					"match": "@@[A-Za-z0-9_]+"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"name": "comment.line.double-slash.cos",
					"match": "//.*$"
				}
			]
		}
	},
	"scopeName": "source.cos"
}